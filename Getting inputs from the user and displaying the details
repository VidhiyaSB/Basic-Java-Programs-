class Details
{
public static void main(String args[])
 { 
String name = args[0];
String dept = args[1];
int roll_no = Integer.parseInt(args[2]); 
int year= Integer.parseInt(args[3]);
System.out.println("Name :"+name);
System.out.println("Department : "+dept);
System.out.println("Roll number: "+roll_no); System.out.println("Year of Birth :"+year);
 }
}

//In Command prompt, 
//To run and compile : javac Details.java
